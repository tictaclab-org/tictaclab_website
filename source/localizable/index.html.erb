---
title: Techies Lab
---

<%= partial "layouts/langbar" %>
<!-- background image -->
<div class="image-bg">
  <div class="bg-filter"></div>
</div>

<!-- Main -->
<section class="main text-center" id="home">
  <div class="page">
    <div class="wrapper">
      <div class="container">
        <%= image_tag "logo/robot.png", alt: "Logo", class: "main-logo" %>
        <h1 class="heading"><%= t('.header.title') %><br><span id="name">Techies Lab</span></h1>
        <p id="tagline">
          <span><%= t('.header.subtitle') %></span>
        </p>
        <!-- Subscription form -->
        <form class="signup" action="//techieslab.us11.list-manage.com/subscribe/post-json?u=4e5edbd56b4da2771ce53024c&amp;id=1c0ef6f860&c=?" role="form" id="signupForm">
          <div class="input-group" style="margin: 0 auto;">
            <div class="form-group pull-left">
              <input type="email" name="EMAIL" class="form-control" placeholder="<%= t('.header.email') %>">
            </div>
            <button type="submit" class="btn btn-theme ladda-button" data-style="expand-left"><span class="ladda-label"><%= t('.header.button.notification') %></span></button>
          </div>
          <div id="mce-responses" class="clear">
            <div id="mce-error-response" class="response alert alert-danger alert-dismissible" style="display:none" role="alert">
              <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <span class="answer"></span>
            </div>
            <div id="mce-success-response" class="response alert alert-success alert-dismissible" style="display:none" role="alert">
              <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <span class="answer"></span>
            </div>
          </div>
          <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
          <div style="position: absolute; left: -5000px;"><input type="text" name="b_4e5edbd56b4da2771ce53024c_1c0ef6f860" tabindex="-1" value="">
          </div>
          <div class="clear"></div>
        </form>
        <footer class="text-center">
          <div class="social">
            <a href="https://twitter.com/TechiesLab" class="btn btn-theme"><i class="fa fa-twitter"></i></a>
            <a href="https://www.facebook.com/TechiesLabBrussels" class="btn btn-theme"><i class="fa fa-facebook"></i></a>
          </div>
        </footer>
      </div>
      <div class="arrow-down">
        <a class="scroll" href="#about" id="scroll"><i class="fa fa-angle-double-down"></i></a>
      </div>
    </div>
  </div>
</section>

<!-- About -->
<section id="about" class="section">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h3 class="section-title"><%= t('.about.title') %></h3>
        <div class="section-title-border"></div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <p class="lead">
          <%= t('about.lead.intro') %> <%= t('about.lead.drop') %> <%= t('about.lead.text') %>
        </p>
        <p class="lead"><%= t('about.intro') %>
        </p>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12 col-sm-6">
        <%= image_tag "featured/techiesLab_dad_son.png", alt: "Dad and Son Coding" %>
      </div>
      <div class="col-xs-12 col-sm-6">
      <%= image_tag "featured/techiesLab_teaching.png", alt: "Female Coach Teaching" %>
      </div>
    </div>
  </div>


</section>



<!-- Poll -->
<section id="about" class="section">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h3 class="section-title"><%= t('.poll.title') %></h3>
        <div class="section-title-border"></div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <p class="lead">
          <%= t('poll.text') %>
        </p>
      </div>
    </div>
    <div class="row"><div class="col-sm-12"><div class="button"><%= link_to t('poll.button'), t('poll.link'), target: "_blank", class: "bb btn btn-theme"  %></div></div></div>

  </div>
</section>

